module.exports = {
  slash: false,
  name: "v12-ping",
  description: "Replies with Pong!",
  aliases: ["pong"],
  async execute(interaction) {
    const ping = interaction.client.ws.ping;
    await interaction.reply(`Pong! ${ping}ms.\n` +
      `*Sent the message at: <t:${(interaction.createdTimestamp / 1000).toFixed()}:F>*`)
  },
};
